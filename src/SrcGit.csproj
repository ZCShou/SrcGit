<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>net6.0-windows</TargetFrameworks>
        <OutputType>WinExe</OutputType>
        <UseWPF>true</UseWPF>
        <ApplicationIcon>Resources\App.ico</ApplicationIcon>
        <Company>SrcGit</Company>
        <Description>OpenSource GIT client for Windows</Description>
        <Copyright>Copyright Â© SrcGit 2022. All rights reserved.</Copyright>
        <ApplicationManifest>App.manifest</ApplicationManifest>
        <Version>6.0</Version>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <StartupObject>SrcGit.App</StartupObject>
        <PackageProjectUrl>https://gitee.com/zcshou/SrcGit.git</PackageProjectUrl>
        <RepositoryUrl>https://gitee.com/zcshou/SrcGit.git</RepositoryUrl>
        <RepositoryType>Public</RepositoryType>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <SatelliteResourceLanguages>none</SatelliteResourceLanguages>
        <Platforms>x64;x86</Platforms>
        <PlatformTarget>x64</PlatformTarget>
    </PropertyGroup>
    <ItemGroup>
      <Content Include="Resources\App.ico" />
    </ItemGroup>
</Project>